var geoCoordMap = {
  "学十一公寓": [116.362219, 39.970499],
  "学十公寓": [116.363221, 39.97035],
  "学八公寓": [116.363216, 39.969331],
  "学九公寓": [116.362264, 39.970098],
  "综合食堂": [116.363171, 39.969745],
  "学三公寓": [116.362444, 39.968715],
  "学一公寓": [116.362412, 39.968236],
  "学二公寓": [116.363261, 39.968304],
  "学生食堂": [116.365682, 39.969182],
  "学二十九公寓": [116.367191, 39.968712],
  "教四楼": [116.362879, 39.967723],
  "主楼": [116.365004, 39.967153],
  "教二楼": [116.36464, 39.966341],
  "学六公寓": [116.364793, 39.970516],
  "科研大楼": [116.365718, 39.970101],
  "教一楼": [116.364546, 39.967751],
  "学五公寓": [116.362381, 39.969317],
  "学四公寓": [116.363257, 39.968723],



};

var convertData = function(data) {
  var res = [];
  for (var i = 0; i < data.length; i++) {
    var geoCoord = geoCoordMap[data[i].name];
    if (geoCoord) {
      res.push({
        name: data[i].name,
        value: geoCoord.concat(data[i].value)
      });
    }
  }
  return res;
};
console.log(convertData(data))
