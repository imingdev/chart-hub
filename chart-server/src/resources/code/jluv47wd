option = {
  title: {
    text: '61路下行公交车',
  },
  toolbox: {
    show: true,
    feature: {
      saveAsImage: {
        show: true,
        excludeComponents: ['toolbox'],
        pixelRatio: 2
      }
    }
  },
  legend: {
    data: ['上客人数'],
    left: 'right'
  },
  tooltip: {
    position: 'top',
    formatter: function(params) {
      return params.value[2] + ' commits in ' + hours[params.value[0]] + ' of ' + days[params.value[1]];
    }
  },
  grid: {
    left: 2,
    bottom: 10,
    right: 10,
    containLabel: true
  },
  xAxis: {
    type: 'category',
    data: hours,
    boundaryGap: false,
    splitLine: {
      show: true,
      lineStyle: {
        color: '#999',
        type: 'dashed'
      }
    },
    axisLine: {
      show: false
    }
  },
  yAxis: {
    type: 'category',
    data: days,
    axisLine: {
      show: false
    }
  },
  series: [{
    name: '上客人数',
    type: 'scatter',
    symbolSize: function(val) {
      return val[2] * 2;
    },
    data: data,
    animationDelay: function(idx) {
      return idx * 5;
    }
  }]
};